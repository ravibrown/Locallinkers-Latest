          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="DataAccess.LocalLinkersEntities, DataAccess" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <_DestinationType>AzureWebSite</_DestinationType>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)LocalLinkersEntities-Web.config Connection String">
      <ParameterValue>metadata=res://*/LocalLinkers.csdl|res://*/LocalLinkers.ssdl|res://*/LocalLinkers.msl;provider=System.Data.SqlClient;provider connection string="Data Source=tcp:locallinkers.database.windows.net,1433;Initial Catalog=locallinkers;user id=locallinkers@locallinkers;pwd=Password1!;Integrated Security=False;MultipleActiveResultSets=True;Application Name=EntityFramework"</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
